        <offsets>
            <pitch-deg>-2.66837</pitch-deg>
            <heading-deg>1.78319</heading-deg>
            <roll-deg>-20.0271</roll-deg>
            <x-m>-0.561803</x-m>
            <y-m>0.17816</y-m>
            <z-m>0.38337</z-m>
        </offsets>
